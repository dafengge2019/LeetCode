class Solution(object):
    def sortSentence(self, s):
        """
        :type s: str
        :rtype: str
        """
        a=s.split()
        print (a)
        b=1
        c=[]
        while b<= len(a):
            for i in range (len(a)):
                if b ==int(a[i][-1]):
                    c.append(a[i][:-1])
            b=b+1
            # a.sort ((int(a[i][-1])))     
       
        d = ' '.join(c)
        return d
