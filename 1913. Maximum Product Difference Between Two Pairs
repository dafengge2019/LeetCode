class Solution(object):
    def maxProductDifference(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """

         
        a=min(nums)

        b=max(nums)
 
        nums.remove(a)
        nums.remove(b)
        c=min(nums)
        d=max(nums)

        return (b*d-a*c)
